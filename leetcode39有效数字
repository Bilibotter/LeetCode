# 超越97.5%
class Solution:
    def isNumber(self, s):
        """
        :type s: str
        :rtype: bool
        """
        try:
            s = float(s)
            return True
        except:
            return False

# 由于leetcode重复会扣分的机制
# 只能取巧得到超越72.9%的分数
class Solution:
    def isNumber(self, s):
        """
        :type s: str
        :rtype: bool
        """
        try:
            float(s[:])
            return True
        except:
            return False
